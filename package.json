{
  "name": "nestjs-starter-rest-api",
  "private": true,
  "version": "1.0.0",
  "description": "NestJS Starter REST API Repository",
  "author": "Saqib Rao",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --preserveWatchOutput",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "cli:dev": "ts-node -r tsconfig-paths/register src/cli.ts",
    "cli:prod": "node dist/src/cli.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "typeorm-ts-node-commonjs -d ./ormconfig.ts",
    "migration:generate": "ts-node node_modules/.bin/typeorm migration:generate -d ormconfig.ts migrations/$npm_config_name",
    "migration:run": "ts-node node_modules/.bin/typeorm migration:run -d ormconfig.ts",
    "migration:revert": "ts-node node_modules/.bin/typeorm migration:revert -d ormconfig.ts",
    "doc:serve": "npx compodoc -p tsconfig.json -w -s -r 8001",
    "doc:generate": "npx compodoc -p tsconfig.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@fastify/static": "^8.1.1",
    "@nestjs/common": "^11.0.15",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.15",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.0.16",
    "@nestjs/swagger": "^11.1.1",
    "@nestjs/typeorm": "^11.0.0",
    "@types/uuid": "^10.0.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.5.0",
    "joi": "^17.13.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.14.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "5.0.1",
    "typeorm": "^0.3.22",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@compodoc/compodoc": "^1.1.26",
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.0.15",
    "@types/bcrypt": "^5.0.2",
    "@types/node": "^22.14.0",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "supertest": "^7.1.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}
